{
  "name": "@jtan15010/dash",
  "version": "2.0.0",
  "description": "Dash - Agent Orchestration Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "dash": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:performance": "ts-node tests/performance/benchmark.ts",
    "test:perf:baseline": "ts-node tests/performance/benchmark.ts --scenario baseline",
    "test:perf:standard": "ts-node tests/performance/benchmark.ts --scenario standard",
    "test:perf:full": "ts-node tests/performance/benchmark.ts --scenario full",
    "test:integration:db": "jest --testPathPattern='database'",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "quality": "npm run lint && npm run typecheck && npm test",
    "migrate": "ts-node scripts/migrate.ts",
    "migrate:status": "ts-node scripts/migrate.ts status",
    "migrate:create": "ts-node scripts/migrate.ts create",
    "migrate:rollback": "ts-node scripts/migrate.ts rollback",
    "db:up": "docker-compose up -d postgres redis",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f postgres redis",
    "logs": "ts-node scripts/logs.ts",
    "logs:query": "ts-node scripts/logs.ts query",
    "logs:tail": "ts-node scripts/logs.ts tail",
    "logs:stats": "ts-node scripts/logs.ts stats",
    "logs:agents": "ts-node scripts/logs.ts agents",
    "monitoring:up": "docker-compose -f monitoring/docker-compose.yml up -d",
    "monitoring:down": "docker-compose -f monitoring/docker-compose.yml down",
    "monitoring:logs": "docker-compose -f monitoring/docker-compose.yml logs -f",
    "monitoring:status": "docker-compose -f monitoring/docker-compose.yml ps",
    "loki:ready": "curl -s http://localhost:3100/ready",
    "grafana:open": "open http://localhost:3000"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.5",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-async-hooks": "^2.5.0",
    "@opentelemetry/core": "^2.5.0",
    "@opentelemetry/exporter-jaeger": "^2.5.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.211.0",
    "@opentelemetry/instrumentation-http": "^0.211.0",
    "@opentelemetry/instrumentation-pg": "^0.63.0",
    "@opentelemetry/instrumentation-redis": "^0.59.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/sdk-trace-node": "^2.5.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@sinclair/typebox": "^0.34.48",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/ws": "^8.18.1",
    "async-mutex": "^0.5.0",
    "better-sqlite3": "^12.6.2",
    "blessed": "^0.1.81",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "fastify": "^5.7.4",
    "glob": "^10.3.0",
    "ioredis": "^5.9.2",
    "jiti": "^2.6.1",
    "pg": "^8.13.1",
    "pg-pool": "^3.7.0",
    "prom-client": "^15.1.3",
    "ws": "^8.19.0",
    "yaml": "^2.5.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/blessed": "^0.1.25",
    "@types/glob": "^8.1.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.10",
    "@types/pg-pool": "^2.0.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
