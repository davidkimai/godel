{
  "dashboard": {
    "id": null,
    "title": "Dash Orchestrator Overview",
    "tags": ["dash", "orchestrator"],
    "timezone": "browser",
    "schemaVersion": 38,
    "refresh": "15s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Active Swarms",
        "type": "stat",
        "targets": [
          {
            "expr": "dash_swarms_active",
            "legendFormat": "Active Swarms"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Active Agents",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(dash_agents_active)",
            "legendFormat": "Active Agents"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 }
      },
      {
        "id": 3,
        "title": "Failed Agents",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(dash_agents_failed)",
            "legendFormat": "Failed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "yellow" },
                { "value": 5, "color": "red" }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 }
      },
      {
        "id": 4,
        "title": "Total Cost (USD)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(dash_swarm_cost_usd)",
            "legendFormat": "Total Cost"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 }
      },
      {
        "id": 5,
        "title": "Avg Budget Utilization",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(dash_budget_utilization_ratio)",
            "legendFormat": "Budget Used"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.75, "color": "yellow" },
                { "value": 0.9, "color": "red" }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 }
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "dash_memory_usage_bytes{type=\"rss\"} / 1024 / 1024",
            "legendFormat": "RSS (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decmbytes",
            "min": 0
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 }
      },
      {
        "id": 7,
        "title": "Agent Status by Swarm",
        "type": "timeseries",
        "targets": [
          {
            "expr": "dash_agents_active",
            "legendFormat": "Active - {{swarm_id}}"
          },
          {
            "expr": "dash_agents_pending",
            "legendFormat": "Pending - {{swarm_id}}"
          },
          {
            "expr": "dash_agents_failed",
            "legendFormat": "Failed - {{swarm_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
      },
      {
        "id": 8,
        "title": "Event Processing Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(dash_events_total[5m])",
            "legendFormat": "{{event_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 }
      },
      {
        "id": 9,
        "title": "API Request Latency (p95)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(dash_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 }
      },
      {
        "id": 10,
        "title": "Agent Execution Time",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(dash_agent_execution_duration_seconds_bucket[5m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 }
      },
      {
        "id": 11,
        "title": "Swarm Success vs Failure",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(dash_swarm_success_total[5m])",
            "legendFormat": "Success - {{strategy}}"
          },
          {
            "expr": "rate(dash_swarm_failure_total[5m])",
            "legendFormat": "Failure - {{strategy}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 }
      },
      {
        "id": 12,
        "title": "Error Rate by Component",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(dash_errors_total[5m])",
            "legendFormat": "{{component}} - {{error_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 }
      },
      {
        "id": 13,
        "title": "Cost by Swarm",
        "type": "table",
        "targets": [
          {
            "expr": "dash_swarm_cost_usd",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "indexByName": {},
              "renameByName": {
                "swarm_id": "Swarm ID",
                "Value": "Cost (USD)"
              }
            }
          }
        ],
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 }
      }
    ]
  }
}
