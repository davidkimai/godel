{
  "dashboard": {
    "id": null,
    "uid": "godel-error-tracking",
    "title": "Godel - Error Tracking",
    "tags": ["godel", "errors", "reliability", "debugging"],
    "timezone": "browser",
    "schemaVersion": 38,
    "refresh": "15s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "component",
          "type": "query",
          "query": "label_values(dash_errors_total, component)",
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "severity",
          "type": "query",
          "query": "label_values(dash_errors_total, severity)",
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(dash_errors_total[5m]))",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.1, "color": "yellow" },
                { "value": 1, "color": "red" }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Total Errors (1h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(dash_errors_total[1h]))",
            "legendFormat": "Total Errors"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 }
      },
      {
        "id": 3,
        "title": "Agent Failure Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(dash_agent_failures_total[5m]))",
            "legendFormat": "Failures/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 }
      },
      {
        "id": 4,
        "title": "Error % of Events",
        "type": "gauge",
        "targets": [
          {
            "expr": "(sum(rate(dash_errors_total[5m])) / sum(rate(dash_events_total[5m]))) * 100",
            "legendFormat": "Error %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "yellow" },
                { "value": 5, "color": "red" }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 }
      },
      {
        "id": 5,
        "title": "Events Dropped",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(dash_events_dropped_total[5m]))",
            "legendFormat": "Dropped/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.01, "color": "yellow" },
                { "value": 0.1, "color": "red" }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 }
      },
      {
        "id": 6,
        "title": "Error Budget Remaining",
        "type": "gauge",
        "targets": [
          {
            "expr": "error_budget:dash_monthly:remaining_ratio * 100",
            "legendFormat": "Budget %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 25, "color": "orange" },
                { "value": 50, "color": "yellow" },
                { "value": 75, "color": "green" }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 }
      },
      {
        "id": 7,
        "title": "Error Rate Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(dash_errors_total[5m]))",
            "legendFormat": "Total Error Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
      },
      {
        "id": 8,
        "title": "Error Rate by Component",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (component) (rate(dash_errors_total[5m]))",
            "legendFormat": "{{component}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 }
      },
      {
        "id": 9,
        "title": "Errors by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (error_type) (increase(dash_errors_total[1h]))",
            "legendFormat": "{{error_type}}"
          }
        ],
        "options": {
          "legend": { "displayMode": "table", "placement": "right" }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 12 }
      },
      {
        "id": 10,
        "title": "Errors by Severity",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (severity) (increase(dash_errors_total[1h]))",
            "legendFormat": "{{severity}}"
          }
        ],
        "options": {
          "legend": { "displayMode": "table", "placement": "right" }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 12 }
      },
      {
        "id": 11,
        "title": "Agent Failures by Reason",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (failure_reason) (increase(dash_agent_failures_total[1h]))",
            "legendFormat": "{{failure_reason}}"
          }
        ],
        "options": {
          "legend": { "displayMode": "table", "placement": "right" }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 12 }
      },
      {
        "id": 12,
        "title": "Agent Failures by Team",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (swarm_id) (increase(dash_agent_failures_total[1h]))",
            "legendFormat": "{{swarm_id}}"
          }
        ],
        "options": {
          "legend": { "displayMode": "table", "placement": "right" }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 12 }
      },
      {
        "id": 13,
        "title": "API Error Rate by Endpoint",
        "type": "timeseries",
        "targets": [
          {
            "expr": "(sum by (method, route) (rate(dash_api_request_duration_seconds_count{status_code=~\"5..\"}[5m])) / sum by (method, route) (rate(dash_api_request_duration_seconds_count[5m]))) * 100",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 }
      },
      {
        "id": 14,
        "title": "Team Failure Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "(sum(rate(dash_swarm_failure_total[5m])) / (sum(rate(dash_swarm_success_total[5m])) + sum(rate(dash_swarm_failure_total[5m])))) * 100",
            "legendFormat": "Failure Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 }
      },
      {
        "id": 15,
        "title": "Error Budget Burn Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "error_budget:dash_daily:burn_rate",
            "legendFormat": "Daily Burn Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 }
      },
      {
        "id": 16,
        "title": "Error Budget Consumption Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "error_budget:dash_monthly:consumed",
            "legendFormat": "Consumed"
          },
          {
            "expr": "error_budget:dash_monthly:allowed",
            "legendFormat": "Allowed (Monthly)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 10
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 }
      },
      {
        "id": 17,
        "title": "Top Error Sources (Last Hour)",
        "type": "table",
        "targets": [
          {
            "expr": "topk(20, sum by (component, error_type) (increase(dash_errors_total[1h])))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "component": "Component",
                "error_type": "Error Type",
                "Value": "Count (1h)"
              },
              "indexByName": {
                "Component": 0,
                "Error Type": 1,
                "Count (1h)": 2
              }
            }
          },
          {
            "id": "sortBy",
            "options": {
              "sort": [{ "field": "Count (1h)", "desc": true }]
            }
          }
        ],
        "gridPos": { "h": 10, "w": 24, "x": 0, "y": 36 }
      }
    ]
  }
}
