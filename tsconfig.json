{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["ES2020"],
    "outDir": "./dist",
    "rootDir": ".",
    "strict": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": false,
    "noPropertyAccessFromIndexSignature": true,
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@godel/ai": ["./packages/ai/src/index.ts"]
    },
    "types": ["node", "react"]
  },
  "include": ["src/**/*", "packages/*/src/**/*"],
  "exclude": ["node_modules", "dist", "tests", "**/*.test.ts", "**/*.spec.ts", "packages/**/__tests__", "src/dashboard/ui", "src/__mocks__"]
}
